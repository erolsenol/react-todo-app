{
  "extends": [
    "react-app",
    "react-app/jest",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "react/jsx-indent": ["error", 2, 
      {
        "checkAttributes": true, "indentLogicalExpressions": true
      }
    ]
  }
}